<h2>회원가입</h2>

<%= form_with model: @user do |f|%>

<%= f.label :nickname %>
<%= f.text_field :nickname %>
<br>
<%= f.label :identifier %>
<%= f.text_field :identifier %>
<br>
<%= f.label :password %>
<%= f.password_field :password %>
<br>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
<br>
<%= f.submit %>
<% end %>